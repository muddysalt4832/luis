    loadstring(game:HttpGet('https://pastebin.com/raw/W5aQUui6'))()
end)

MainSection:NewButton("Admin", "Gives you Admin menu", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/c9skYnFn'))()
end)

MainSection:NewButton("God Mode", "Gives you god mode", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/Wjby1EEv'))()
end)

MainSection:NewButton("Pet Sim X", "Pet Sim X Auto Hatch", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/epgnNu91'))()
end)
